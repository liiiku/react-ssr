{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "简单的react开发环境搭建 服务端渲染 开发环境的服务端渲染",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "node server/server.js",
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "build:test": "webpack -w --inline --config build/webpack.config.client.js",
    "build": "npm run clean && npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liiiku/react-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liiiku/react-ssr/issues"
  },
  "homepage": "https://github.com/liiiku/react-ssr#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.3.6",
    "rimraf": "^2.6.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.4"
  }
}
